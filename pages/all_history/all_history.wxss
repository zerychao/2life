.container{
  background-color:#2B293D;
  height: 100%;
  position:absolute;
  top:0px;
  left:0px;
  right:0px;
  bottom:0px;
  z-index: -1;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top:20px;
  width: 100%;
  height: 100px;
}

.userinfo-avatar {
  width: 64px;
  height: 64px;
  border-radius: 50%;
}

.userinfo-nickname {
  width: 88px;
  height: 18px;
  font-family: Avenir Next;
  font-size: 12px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #ffffff;
  margin-top: 8px;
}
.dairyContainer{
  position: absolute;
  top: 105px;
  left: 0px;
  right: 0px;
  bottom: 0px;
}

 .scrollview{
  display: inline-block;
  height: 300px;
} 
.dairyid{
  width: 40px;
  height: 40px;
  background-color: #3d3a55;
  box-shadow: inset 0 3px 3px 0 rgba(0, 0, 0, 0.5);
  border-radius: 50%;
}
.dairyid text{
  width: 46px;
  height: 25px;
  font-family: AvenirNext;
  font-size: 18px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #a8a1e5;
}
.dairytitle{
  width: 120px;
  height: 140px;
  border-radius: 8px;
  background-color: #fafafa;
}

.dairytitle text{
  width: 100px;
  height: 90px;
  font-family: AvenirNext;
  font-size: 16px;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: #000000;
}

.testy{
  width: 100%;
  height: 100%;
}
.testx{
  width: 100%;
  height: 100%;
}